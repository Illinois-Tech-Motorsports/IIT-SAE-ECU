{
    /*
     * |-----[ CONFIGURATION ]-----|
     */
    // Serial Monitor
    "FRONT_TEENSY_PORT": "COM3", // Port representing the front teensy
    "BACK_TEENSY_PORT": "COM16", // Port representing the back teensy
    "BAUDRATE": "115200", // Baudrate to use with serial
    "GRAPH_ARG": "-g", // "-g" to enable plotting of data, else ""
    // Teensy
    "CORE_MODEL": "41", // Model number of the teensy to compile for (36, 40, 41)
    "CORE_NAME": "MK66FX1M0", // Model of the cpu (only used for IDE)
    "CORE_SPEED": "180000000", // Speed at which the CPU will run (Hz)
    "USB_SETTING": "USB_SERIAL", // USB behavior of the core
    // Toolchain
    "TOOLCHAIN_OFFSET": "../TeensyToolchain", // Offset to the toolchain
    "ADDITIONAL_CMAKE_VARS": "-DCUSTOM_BUILD_PATH_PREFIX:STRING=build/Pre_Build/", // More defines passed to CMake
    /*
     * |---[ END CONFIGURATION ]---|
     */
    "CMAKE_FINAL_VARS": "-DENV_CORE_SPEED:STRING=${config:CORE_SPEED} -DENV_CORE_MODEL:STRING=${config:CORE_MODEL} -DENV_USB_SETTING:STRING=${config:USB_SETTING} ${config:ADDITIONAL_CMAKE_VARS}",
    "files.associations": {
        "*.def": "cpp",
        "*.tcc": "cpp",
        "*.tpp": "cpp",
        "cstdint": "cpp",
        "algorithm": "cpp",
        "system_error": "cpp",
        "random": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "array": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "exception": "cpp",
        "functional": "cpp",
        "type_traits": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cinttypes": "cpp",
        "utility": "cpp",
        "map": "cpp",
        "*.obj": "cpp",
        "string": "cpp",
        "queue": "cpp",
        "atomic": "cpp",
        "memory": "cpp",
        "optional": "cpp",
        "set": "cpp",
        "bit": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "iterator": "cpp",
        "memory_resource": "cpp",
        "string_view": "cpp",
        "ranges": "cpp"
    },
    "doxygen_runner.configuration_file_override": "${workspaceFolder}\\docs\\Doxyfile",
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": false,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4 }",
    "search.exclude": {
        "**/build": true
    },
    "files.watcherExclude": {
        "**/build": true
    },
}